<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Courses</title>
    <link rel="stylesheet" href="./stylesheets/courseListStyle.css"> <!-- Link to your CSS file -->
</head>
<body>
    <h1>Courses</h1>
    <ul>
        <% courses.forEach(course => { %>
            <li>
                <a href="<%= `courses/chapter${course.id}`%>"><%= course.title %></a>
                <p><%= course.description %></p>
                <p>Level: <%= course.level %></p>
            </li>
        <% }) %>
    </ul>
</body>
</html>
